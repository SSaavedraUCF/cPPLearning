
#include<iostream>

using namespace std;

int main()
{
    int num1{},num2{};
    
    cout<<boolalpha;
    cout<<"Enter 2 Numbers";
    cin>>num1>>num2;

    cout<<"Num1 is Greater than Num2:"<< (num1>num2)<<endl;
    cout<< "Num1 is equal to Num2:"<<(num1==num2)<<endl;
    cout<<"Num1 is Less than or Equal to Num 2:" (num1<=num2)<<endl;
    return 0;
}